<div class="row">
<%= render 'books/book', book: @book, user: @user %>

<div class="col-8 offset-1">
<h1>Users</h1>

<table class="table">
  <tr>
    <th>image</th>
    <th>name</th>
  </tr>
  
 <% @books.each do |book| %>
  <tr>
    <td>
      <%= book.image %>
    </td>
    <td>
      <%= book.name %>
    </td>
 </tr>
 <% end %>
</table>
</div>
</div>
